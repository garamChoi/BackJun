#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>
#include<stdlib.h>

int main(void)
{
	int n;
	int i, j;

	scanf("%d", &n);

	for (i = 1; i <= n; i++)
	{                          
		for (j = 1; j <= i; j++)   // print out star symbols by order
			printf("*");
		printf("\n");     
	}

	return 0;
}
